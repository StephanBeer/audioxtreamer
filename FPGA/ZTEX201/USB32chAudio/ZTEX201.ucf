NET "ymh_clk"	LOC = "K14" | IOSTANDARD = LVCMOS33 ;		# A14 / K14~IO_L41N_GCLK8_M1CASN_1

TIMESPEC "TS_XDOMAINS_iistousb" = FROM "ymh_clk" TO "cy_clk" TIG;
