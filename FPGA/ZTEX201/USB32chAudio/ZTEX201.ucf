NET "cy_clk" TNM_NET = "cy_clk";
TIMESPEC "TS_cy_clk" = PERIOD "cy_clk" 48 MHz HIGH 50 %;
NET "cy_clk"  LOC = "T8" | IOSTANDARD = LVCMOS33 ;

# use the 96Khz freq for timing closure but expect it to be half 
